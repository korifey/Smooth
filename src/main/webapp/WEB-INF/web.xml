<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>MainServlet</servlet-name>
        <servlet-class>org.example.navigator.MainServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ObstacleGetServlet</servlet-name>
        <servlet-class>org.example.navigator.obstacle.ObstacleGetServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ObstacleAllServlet</servlet-name>
        <servlet-class>org.example.navigator.obstacle.ObstacleAllServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ObstacleRemoveServlet</servlet-name>
        <servlet-class>org.example.navigator.obstacle.ObstacleRemoveServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ObstacleAddServlet</servlet-name>
        <servlet-class>org.example.navigator.obstacle.ObstacleAddServlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>MainServlet</servlet-name>
        <url-pattern>/path</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ObstacleGetServlet</servlet-name>
        <url-pattern>/obstacle/get</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ObstacleAllServlet</servlet-name>
        <url-pattern>/obstacle/all</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ObstacleRemoveServlet</servlet-name>
        <url-pattern>/obstacle/remove</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ObstacleAddServlet</servlet-name>
        <url-pattern>/obstacle/add</url-pattern>
    </servlet-mapping>
</web-app>
